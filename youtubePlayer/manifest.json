{
  "manifest_version": 3,
  "name": "Youtube style player",
  "version": "5.1.0",
  "description": "This extension change the other player in youtube player",
  "permissions": [
    "storage",
    "scripting",
    "notifications",
    "webNavigation"
  ],
  "host_permissions": [ "<all_urls>" ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
